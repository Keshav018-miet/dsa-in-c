#include <stdio.h>

int main() {
    int N;
    scanf("%d", &N);
    
    int arr[110];
    for(int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }
    
    int X, P;
    scanf("%d", &X);
    scanf("%d", &P);
    for(int i = N; i > P; i--) {
        arr[i] = arr[i-1];
    }
    arr[P] = X;
    N = N + 1;
    for(int i = 0; i < N; i++) {
        printf("%d", arr[i]);
        if(i < N-1) printf(" ");
    }
    printf("\n");
    return 0;
}
