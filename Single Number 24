#include <stdio.h>
int singleNumber(int nums[], int numsSize) {
    int result = 0;
    for (int i = 0; i < numsSize; i++) {
        result = result ^ nums[i];
    }   
    return result;
}
int main() {
    int n;
    scanf("%d", &n);   
    int arr[30001];
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    int answer = singleNumber(arr, n);
    printf("%d\n", answer);   
    return 0;
}
