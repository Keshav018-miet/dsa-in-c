#include<stdio.h>
#include<math.h>
int main (){
    int num, sum=0,rem,p=0;
    scanf("%d",&num);
    int temp = num;
while(temp!=0){
    int rem= temp%10;
    
    if(rem >= 8 || rem <= 0){
        printf("invalid");
        return 1;
    }
    
        sum = sum + rem*pow(8,p);
        p++;
    rem= temp/10;
}
printf("%d",sum);
return 0;
}
